{%- if current.element.getOption("label") -%}
    <label
        id="{{ current.element.getId() }}-label"
        for="{{ current.element.getId() }}"
        {% if current.element.getOption("tooltip") %}data-tooltip aria-haspopup="true" title="{{ current.element.getOption("tooltip") }}" class="has-tip"{% endif %}
        >
        {{ current.element.getOption("label") }}
    </label>
{%- endif -%}
